<h1>Create a New Account</h1>
<%= form_for @user do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %></p>

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %></p>

  <%= f.label :email %>:
  <%= f.text_field :email %></p>

  <%= f.label :university_or_school %>:
  <%= f.text_field :university_or_school %></p>

  <%= f.submit %>
<% end %>
